{
  "name": "newover/sortie",
  "description": "A way out for data.",
  "homepage": "https://github.com/davemedema/sortie",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "type": "library",
  "authors": [
    {
      "name": "Dave Medema",
      "email": "davemedema@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Sortie\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "keywords": [
    "helpers",
    "manipulation",
    "string",
    "utilities",
    "utility",
    "utils"
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0",
    "nesbot/carbon": "^2.67"
  },
  "require-dev": {
    "league/csv": "^9.2",
    "phpunit/phpunit": "^8.5"
  }
}
